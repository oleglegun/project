{
    "extends": ["react-app", "prettier", "plugin:redux-saga/recommended"],
    "env": {
        "jest": true
    },
    "rules": {
        
        "yield-effects": "true",
        // Fixes bug "href-no-hash"
        "jsx-a11y/href-no-hash": "off",
        "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
    }
}
